<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.stay.accommodation.wellihilli.mapper.WellihilliMapper">
    <select id="updateRmtype" resultType="String">
        EXEC PLNDSTAY.dbo.spWHP_RMTYPE_UPDATE #{strRoomDatas}, ''
    </select>

    <select id="getStrRmtypeID" parameterType="int" resultType="String">
        SELECT strRmtypeID
        FROM PLNDSTAY.dbo.RMTYPE
        WHERE intIdx = ${intRmIdx}
    </select>

    <select id="updateGoods" resultType="String">
        EXEC PLNDSTAY.dbo.spONDAnWHP_STOCK_TEMP_REGIST 'WHP', #{strStockDatas}, ''
    </select>
</mapper>