    <select id="accommRegist" parameterType="CondoDto" resultType="String">
        EXEC spGW_CONDO_REGIST_SUPPLIER 0, #{strAccommId}, #{strApiFlag}, #{strAcmName}, #{strZipCode}
        , #{strAddress}, #{strConAddrNew}, #{strConTel}, #{strConFax}, '', '', #{strLoc}, '', #{strTimeIn}, #{strTimeOut}
        , #{strConDisplay}, #{decLng}, #{decLat}, #{strUsageNotice}, #{strCity}, #{strNation}, #{strSummary}, '', '', #{strTagName}, ''
    </select>

    <select id="accommPhotoContentsReg" parameterType="ContentsPhotoDto" resultType="String">
        EXEC spGW_CONTENTS_PHOTO_REGIST_TEST<![CDATA[0, #{strSubject}, #{strFilePath}, #{strFileName}, ${intCreatedSID}, ${intModifiedSID}, #{strCid}, '']]>
    </select>

    <select id="cancelInfoReg" parameterType="CancelInfoDto" resultType="String">
        EXEC spGW_CANCEL_INFO_REGIST_TEST ${intCid},<![CDATA[#{strCname}]]>, #{strCnFlag}, ${intCnDcnt}, ${intCnPer}, ''
    </select>

    <select id="roomTypeRegist" parameterType="ToconDto" resultType="String">
        EXEC spGW_ROOMTYPE_REGIST_SUPPLIER 0, #{tocode}, #{conId}, ${standpeopleCnt}, ${standpeopleCnt}, #{roomTypeId}, ${admin_count}, #{strIngYN}, #{tocodeText}, '', ''
    </select>
